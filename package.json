{
  "name": "videojs-http-source-selector",
  "version": "1.1.7",
  "type": "module",
  "description": "VideoJS plugin that leverages videojs-contrib-quality-levels plugin to offer manual user-selectable level selection options for adaptive http streams.",
  "main": "dist/videojs-http-source-selector.cjs.js",
  "repository": "github:FreeTubeApp/videojs-http-source-selector",
  "homepage": "https://github.com/FreeTubeApp/videojs-http-source-selector",
  "bugs": "https://github.com/FreeTubeApp/videojs-http-source-selector/issues",
  "module": "dist/videojs-http-source-selector.es.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "concurrently \"npm:build:css\" \"npm:build:js\" \"npm:docs\" && npm run add-headers",
    "build:css": "node scripts/copy-css.cjs",
    "build:js":"concurrently \"npm:build:js:*\" && npm run build:uglify",
    "build:js:rollup-modules": "rollup -c scripts/modules.rollup.config.js",
    "build:js:rollup-umd": "rollup -c scripts/umd.rollup.config.js",
    "build:uglify": "uglifyjs dist/videojs-http-source-selector.js --comments --mangle --compress  -o dist/videojs-http-source-selector.min.js",
    "build:test": "rollup -c scripts/test.rollup.config.js",
    "add-headers": "dist-header dist",
    "clean": "rimraf dist test/dist",
    "docs": "jsdoc src -r -c jsdoc.json -d docs/api",
    "lint": "eslint ./src",
    "pretest": "concurrently \"npm:lint\" \"npm:build\"",
    "watch": "concurrently \"npm:watch*",
    "watch:css": "concurrently \"npm:build:css\" \"npm:watch:css:copy\"",
    "watch:css:copy": "node scripts/copy-css.cjs --watch",
    "watch:js-modules": "rollup -c scripts/modules.rollup.config.js -w",
    "watch:js-umd": "rollup -c scripts/umd.rollup.config.js -w",
    "prepublish": "not-in-install && npm run build || in-install"
  },
  "keywords": [
    "videojs",
    "video.js",
    "videojs-plugin",
    "FreeTube",
    "dash",
    "quality selector",
    "addaptive http"
  ],
  "author": "Justin Fujita <Justin@pivotshare.com>",
  "contributors": ["ChunkyProgrammer", "FreeTube Team"],
  "license": "MIT",
  "files": [
    "README.md",
    "dist/",
    "LICENSE",
    "src/"
  ],
  "peerDependencies": {
    "video.js": "^6 || ^7 || ^8"
  },
  "dependencies": {
    "videojs-contrib-quality-levels": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/plugin-transform-object-assign": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^23.0.3",
    "@rollup/plugin-json": "^5.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "add-dist-header": "^0.3.7",
    "concurrently": "^7.6.0",
    "eslint": "^8.29.0",
    "eslint-config-videojs": "^6.0.0",
    "eslint-plugin-jsdoc": "^39.6.4",
    "global": "^4.4.0",
    "in-publish": "^2.0.1",
    "jsdoc": "^4.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.6.0",
    "uglify-js": "^3.17.4",
    "video.js": "^6 || ^7 || ^8"
  }
}
