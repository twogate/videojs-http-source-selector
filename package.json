{
  "name": "videojs-http-source-selector",
  "version": "1.1.7",
  "type": "module",
  "description": "VideoJS plugin that leverages videojs-contrib-quality-levels plugin to offer manual user-selectable level selection options for adaptive http streams.",
  "main": "dist/videojs-http-source-selector.cjs.js",
  "repository": "github:twogate/videojs-http-source-selector",
  "homepage": "https://github.com/twogate/videojs-http-source-selector",
  "module": "dist/videojs-http-source-selector.es.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "concurrently \"npm:build:cs*\" \"npm:build:js\" \"npm:docs\" && npm run add-headers",
    "build:css": "sass src/plugin.scss dist/videojs-http-source-selector.css",
    "build:css:min": "sass src/plugin.scss dist/videojs-http-source-selector.min.css --output-style=compressed --linefeed=lf",
    "build:js": "concurrently \"npm:build:js:*\" && concurrently \"npm:build:uglify:*\"",
    "build:js:rollup-modules": "rollup -c scripts/modules.rollup.config.js",
    "build:js:rollup-umd": "rollup -c scripts/umd.rollup.config.js",
    "build:uglify:modern": "uglifyjs dist/videojs-http-source-selector.js --comments --mangle --compress  -o dist/videojs-http-source-selector.min.js --source-map url=videojs-http-source-selector.min.js",
    "add-headers": "dist-header dist",
    "clean": "rimraf dist test/dist",
    "docs": "jsdoc src -r -c jsdoc.json -d docs/api",
    "lint": "eslint ./src",
    "pretest": "concurrently \"npm:lint\" \"npm:build\"",
    "vercel-setup": "npm run build && node scripts/vercel-setup.cjs",
    "prepublish": "not-in-install && npm run build || in-install"
  },
  "keywords": [
    "videojs",
    "video.js",
    "videojs-plugin",
    "videojs plugin",
    "plugin",
    "FreeTube",
    "dash",
    "quality selector",
    "adaptive http"
  ],
  "author": "Justin Fujita <Justin@pivotshare.com>",
  "contributors": [
    "ChunkyProgrammer",
    "FreeTube Team"
  ],
  "license": "MIT",
  "files": [
    "README.md",
    "dist/",
    "LICENSE",
    "src/"
  ],
  "peerDependencies": {
    "video.js": "^6 || ^7 || ^8",
    "videojs-contrib-quality-levels": "^2 || ^3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-transform-object-assign": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "add-dist-header": "^1.0.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.31.0",
    "eslint-config-videojs": "^6.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^48.0.2",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-sonarjs": "^0.24.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "global": "^4.4.0",
    "in-publish": "^2.0.1",
    "jsdoc": "^4.0.0",
    "sass": "^1.78.0",
    "rimraf": "^5.0.0",
    "rollup": "^4.1.4",
    "uglify-js": "^3.17.4",
    "video.js": "^8",
    "videojs-contrib-quality-levels": "^4.0.0"
  }
}
